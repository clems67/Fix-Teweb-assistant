{
  "manifest_version": 3,
  "name": "Fix TEWEB assistant",
  "version": "1.0",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://teweb.fr.sogeti.com/*"],
      "js": ["main.js"]
    }
  ],
  "action": {
    "default_title": "Fix TEWEB assistant",
    "default_popup": "popup.html"
  }
}
